@using Models;

<div class-"container" style="margin-top: -50px">
    <form method="post">
        <div class="row pt-2 pb-1">
            <div class="col=6" style="text-align:center">
                <h3>Golf Group has no Players</h3>
            </div>
        </div>
        <div class="row" >
        <br/>
            <div class="col-2" style="text-align:center">
                <a asp asp-controller="GolfPlayer" asp-action="Index" asp-route-GGId=@ViewBag.GGId class="btn btn-primary">
                    Add Players to Group
                </a>
            </div>
        </div>

    </form>
</div>
@section Scripts
    {

    @{
        <partial name="_ValidationScriptsPartial" />
    }
}

